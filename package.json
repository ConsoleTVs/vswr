{
  "name": "vswr",
  "version": "1.3.1",
  "description": "Stale-While-Revalidate (SWR) strategy to fetch data in Vue 3",
  "author": "Èrik C. Forés <soc@erik.cat>",
  "repository": "https://github.com/ConsoleTVs/vswr",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --watch --format esm,cjs,iife --legacy-output",
    "build": "tsup src/index.ts --dts --format esm,cjs,iife --legacy-output --minify",
    "prepack": "npm run build",
    "switch:2": "vue-demi-switch 2 vue2",
    "switch:3": "vue-demi-switch 3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || ^3.0.4"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^4.11.2",
    "typescript": "^4.3.4",
    "vue": "^3.0.4"
  },
  "dependencies": {
    "swrev": "^1.9.0",
    "vue-demi": "^0.11.2"
  }
}
